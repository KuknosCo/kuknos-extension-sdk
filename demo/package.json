{
  "name": "kuknos-browser-intent-sdk-demo",
  "version": "1.0.0",
  "description": "Demo as kuknos-browser-intent-sdk consumer",
  "main": "demo.ts",
  "author": "Ryan Chen",
  "license": "MIT",
  "scripts": {
    "add-self:js": "touch node_modules/kuknos-browser-intent-sdk.js && echo \"module.exports = require('../..');\" > node_modules/kuknos-browser-intent-sdk.js",
    "add-self:ts": "touch node_modules/kuknos-browser-intent-sdk.d.ts && echo \"import myLib from '../../';export = myLib;\" > node_modules/kuknos-browser-intent-sdk.d.ts",
    "remove-self:js": "rm node_modules/kuknos-browser-intent-sdk.js",
    "remove-self:ts": "rm node_modules/kuknos-browser-intent-sdk.d.ts",
    "add-self": "npm run add-self:js && npm run add-self:ts",
    "remove-self": "npm run remove-self:js && npm run remove-self:ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4"
  }
}
